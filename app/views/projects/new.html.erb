<div class = "title">
  <h1>プロジェクト作成</h1>
    <div class = "button">
      <%= button_to "戻る", :back, method: :get %>
    </div>
</div>
<hr>
<%= form_with model: @project do |form| %>
  <div class="title">
    <div class ="field">
        <%= form.label :name %>
        <%= form.text_field :name %>
    </div>
    <div class="field">
        <%= form.label :note %>
        <%= form.text_area :note %>
    </div>
  </div>
  <div class="title">
    <div class="field">
      <p>what you share?(limit 5 topic)</p>
      <%= form.label :topic1 %>
      <%= form.text_field :topic1 %>
      <%= form.label :topic2 %>
      <%= form.text_field :topic2 %>
      <%= form.label :topic3 %>
      <%= form.text_field :topic3 %>
      <%= form.label :topic4 %>
      <%= form.text_field :topic4 %>
      <%= form.label :topic5 %>
      <%= form.text_field :topic5 %>
    </div>
    <div id ="preview">

    </div>
  </div>
  <div class = "title">
    <%= form.submit  %>
  </div>
<% end %>
