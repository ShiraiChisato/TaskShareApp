<div class = "title">
  <p>Your Profile</p>
</div>
<div id = "profile">
    <%= image_tag @user.icon.url %>
  <ul>
    <li>name:<%= @user.name %></li>
    <li>e-mail:<%= @user.email %></li>
  </ul>
</div>
<div class = "title">
  <p>Your Projects</p>
  <%=  button_to "new project",new_project_path , method: :get %>
</div>
<div id= "item">
  <% @projects.each do |project| %>
    <%= link_to project.name, project_path(project.id) %>
  <% end %>
</div>